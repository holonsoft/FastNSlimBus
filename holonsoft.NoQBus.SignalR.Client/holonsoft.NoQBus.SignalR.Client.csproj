<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Authors>Alexander Bülow, Christian Vogt</Authors>
    <Company>holonsoft</Company>
    <PackageProjectUrl>https://www.nuget.org/packages/holonsoft.NoQBus.SignalR.Client/</PackageProjectUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Copyright>holonsoft</Copyright>
    <Description>Client part for the SignalR bridge of our asynchronous, fast and fully parallelized message bus.</Description>
    <RepositoryUrl>https://github.com/holonsoft/NoQBus</RepositoryUrl>
    <Version>1.7.0</Version>
    <TargetFrameworks>net6.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <ImplicitUsings>enable</ImplicitUsings>
    <DebugType>Embedded</DebugType>
    <RepositoryType>GIT</RepositoryType>
    <PackageId>holonsoft.NoQBus.SignalR.Client</PackageId>
    <PackageTags>holonsoft;noqbus;messagebus;net6</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\holonsoft.NoQBus.Abstractions\holonsoft.NoQBus.Abstractions.csproj" />
    <ProjectReference Include="..\holonsoft.NoQBus.SignalR.Abstractions\holonsoft.NoQBus.SignalR.Abstractions.csproj" />
    <ProjectReference Include="..\holonsoft.NoQBus\holonsoft.NoQBus.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Autofac" Version="6.3.0" />
    <PackageReference Include="Microsoft.AspNetCore.SignalR.Client" Version="6.0.3" />
    <PackageReference Include="SignalR.Strong" Version="0.3.0" />
    <PackageReference Include="SignalR.Strong.Dynamic" Version="0.3.0" />
    <PackageReference Include="System.Text.Json" Version="6.0.2" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  
</Project>
